pkgname=callsign
pkgver=1.0.2
pkgrel=1
pkgdesc="A small program for finding information about a ham radio based on his callsign, written in ruby, implementing callsign-gem"
arch=(any)
url="http://rubyforge.org/projects/callsign"
license=('BSD')
depends=('ruby' 'callsign-gem')
source=(http://files.rubyforge.vm.bytemark.co.uk/callsign/callsign-$pkgver.tar.gz)
md5sums=('de19b3cdefd67bcba8154ef7b11b4cdc')

build() {
	sudo  mv callsign /usr/bin
}
